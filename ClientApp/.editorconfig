# EditorConfig helps developers define and maintain consistent
# coding styles between different editors and IDEs
# editorconfig.org

root = true

# Apply for all files
[*]
charset = utf-8

[*.{js,jsx,ts,tsx}]
insert_final_newline = true
trim_trailing_whitespace = true

[Jenkinsfile]
indent_style = tab
indent_size = 4
